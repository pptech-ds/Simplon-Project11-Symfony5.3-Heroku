{% extends 'base.html.twig' %}

{% block title %}Hello ListController!{% endblock %}

{% block body %}


<table id="datatablesSimple">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Name</th>
                            <th>Slug</th>
                            <th>Url</th>
                            <th>Image</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Id</th>
                            <th>Name</th>
                            <th>Slug</th>
                            <th>Url</th>
                            <th>Image</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        {% for video in videos %}
                            <tr>
                                <td>{{video.id}}</td>
                                <td>{{video.name}}</td>
                                <td>{{video.slug}}</td>
                                <td>{{video.url}}</td>
                                <td>{{video.image}}</td>
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>


{% endblock %}
